config_version=5

[application]

config/name="XS3D_Climatologico"
config/tags=PackedStringArray("xsoft3d")
run/main_scene="res://(Scenes)/Scn_Sector6.tscn"
config/features=PackedStringArray("4.3", "Mobile")
run/max_fps=60

[autoload]

ENUMS="*res://Systems/#GLOBALS/GDs_ENUMS.gd"
UTILITIES="*res://Systems/#GLOBALS/GDs_UTILITIES.gd"
APPSTATE="*res://Systems/#GLOBALS/GDs_APPSTATE.gd"
SIGNALS="*res://Systems/#GLOBALS/GDs_SIGNALS.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/always_on_top=true
window/size/window_width_override=1536
window/size/window_height_override=864
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="XS3D_Climatologico"

[editor_plugins]

enabled=PackedStringArray("res://addons/3DAutoMaterial/plugin.cfg", "res://addons/3DGizmos/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/coi_serviceworker/plugin.cfg")

