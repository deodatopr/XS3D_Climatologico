; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="XS3D_Climatologico"
run/main_scene="res://LVLS/Lvl_Main.tscn"
config/features=PackedStringArray("4.3", "Mobile")

[autoload]

ENUMS="*res://SYSTEMS/GDs_ENUMS.gd"
UTILITIES="*res://SYSTEMS/GDs_UTILITIES.gd"
APPSTATE="*res://SYSTEMS/GDs_APPSTATE.gd"
SIGNALS="*res://SYSTEMS/GDs_SIGNALS.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1536
window/size/window_height_override=864
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=1
window/vsync/vsync_mode=0
window/ios/allow_high_refresh_rate=false
window/ios/hide_status_bar=false

[editor_plugins]

enabled=PackedStringArray("res://addons/3DAutoMaterial/plugin.cfg", "res://addons/3DGizmos/plugin.cfg", "res://addons/3DLightOccluder/plugin.cfg", "res://addons/3DMeshCombiner/plugin.cfg", "res://addons/3DVertexToPath/plugin.cfg", "res://addons/LittleCameraPreview/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/addons/SmoothScroll/plugin.cfg", "res://addons/coi_serviceworker/plugin.cfg", "res://addons/jigglebones/plugin.cfg", "res://addons/snappy/plugin.cfg")

[file_customization]

folder_colors={
"res://#DeosTools/": "red",
"res://#Keystores/": "red",
"res://#NoShare/": "red",
"res://#Studies/": "red",
"res://CONTENT/": "green",
"res://LVLS/": "purple",
"res://SYSTEMS/": "orange",
"res://UI/": "yellow",
"res://addons/": "blue"
}

[input_devices]

pointing/emulate_touch_from_mouse=true
