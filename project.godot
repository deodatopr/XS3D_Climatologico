; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="XS3D_Climatologico"
config/tags=PackedStringArray("xsoft3d")
run/main_scene="res://(Scenes)/Main.tscn"
config/features=PackedStringArray("4.3", "Mobile")
run/max_fps=60

[audio]

buses/default_bus_layout="res://Cont_Sounds/Bus_Layout.tres"

[autoload]

ENUMS="*res://Systems/#GLOBALS/GDs_ENUMS.gd"
UTILITIES="*res://Systems/#GLOBALS/GDs_UTILITIES.gd"
APPSTATE="*res://Systems/#GLOBALS/GDs_APPSTATE.gd"
SIGNALS="*res://Systems/#GLOBALS/GDs_SIGNALS.gd"
CAM="*res://Systems/#GLOBALS/GDs_CAMSTATE.gd"
DEBUG="*res://Systems/#GLOBALS/GDs_DEBUGSTATE.gd"
CONST="*res://Systems/#GLOBALS/GDs_CONSTANTS.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/size/always_on_top=true
window/size/window_width_override=1536
window/size/window_height_override=864
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="XS3D_Climatologico"

[editor_plugins]

enabled=PackedStringArray("res://addons/3DAutoMaterial/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/coi_serviceworker/plugin.cfg")

[file_customization]

folder_colors={
"res://#NoShare/": "red",
"res://#Resources/": "red",
"res://#Resources/#AndroidKeystores/": "red",
"res://#Wip/": "red",
"res://(Scenes)/": "gray",
"res://Cont_Models/": "green",
"res://Cont_Models/#Shared/": "green",
"res://Cont_Sounds/": "pink",
"res://Cont_UI/": "yellow",
"res://Content/": "green",
"res://PPE/": "purple",
"res://SYSTEMS/z_EndPoint Old/": "gray",
"res://SYSTEMS/z_Graficadora/": "gray",
"res://SYSTEMS/z_OclusionByHeight/": "gray",
"res://SYSTEMS/z_Scn_Filtros/": "gray",
"res://Shaders/": "purple",
"res://Sounds/": "pink",
"res://Systems/": "orange",
"res://UI/": "yellow",
"res://Vfx_PostProces/": "purple",
"res://Vfx_Shaders/": "purple",
"res://addons/": "blue"
}

